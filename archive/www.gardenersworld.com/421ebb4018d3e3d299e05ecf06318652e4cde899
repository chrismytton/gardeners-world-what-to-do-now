    <script type="text/javascript">
        
                        var seasonImages = new Array();
                        seasonImages[0] = '/how-to/images/Sping1.png';
                        seasonImages[1] = '/how-to/images/Summer1.png';
                        seasonImages[2] = '/how-to/images/Autumn1.png';
                        seasonImages[3] = '/how-to/images/Winter1.png';
                        var currentSeason = 0;
        
        var viewport = $j('.calendar-carousel-week-viewport');
        var viewportWidth = $j('.calendar-carousel-week-viewport').width();
        var weeksContainer = $j('.calendar-carousel-week-item-container');
        var itemWidth = $j(".calendar-carousel-week-item-container :first-child").outerWidth(true);
        var max = 0;
        var min = viewportWidth - weeksContainer.width();
        var itemsVisible = viewportWidth / itemWidth;
        var numberToScroll = parseInt(itemsVisible / 2);

        $j('.calendar-carousel-next').click(function () {
            var currentx = weeksContainer.offset().left - viewport.offset().left;
            var newx = currentx - (numberToScroll * itemWidth);
            newx = Math.min(Math.max(newx, min), max);
            weeksContainer.animate({
                left: newx
            }, 500, function () {
                checkBackgroundImage(viewport, weeksContainer, itemWidth);
            });
        });

        $j('.calendar-carousel-previous').click(function () {
            var currentx = weeksContainer.offset().left - viewport.offset().left;
            var newx = currentx + (numberToScroll * itemWidth);
            newx = Math.min(Math.max(newx, min), max);
            weeksContainer.animate({
                left: newx
            }, 500, function () {
                checkBackgroundImage(viewport, weeksContainer, itemWidth);
            });
        });

        function checkBackgroundImage(viewport, weeksContainer, itemWidth) {
            /*
            var currentx = Math.abs(weeksContainer.offset().left - viewport.offset().left);
            var index = parseInt((currentx / itemWidth)) % 52;
            // update to the 2nd item in the list (in place of this week)

            var firstItem = $j(weeksContainer).children(':first-child');

            var startWeek = parseInt(firstItem.attr('id').replace('week-', ''));

            index += startWeek;
            var item = $j('#week-' + index);
            var newSeason = parseInt(item.attr('data-season'));
            if (newSeason != currentSeason) {
                // create a new div and fade in
                var seasonBackgroundImageUrl = seasonImages[newSeason];
                var backgroundDiv = $j('<div class="calendar-background-image-container" style="background-image: url(' + seasonBackgroundImageUrl + ');" id="season-' + newSeason + '"></div>');
                var backgroundContainer = $j('#calendar-background');
                backgroundContainer.append(backgroundDiv);
                backgroundDiv.fadeIn(1000);
                var existing = $j('#season-' + currentSeason);
                if (existing.length > 0) {
                    existing.fadeOut(1000, function () {
                        $j(this).remove();
                    });
                }
                currentSeason = newSeason;
            }
            */
        }


    </script>



